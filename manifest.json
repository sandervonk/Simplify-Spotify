{
  "manifest_version": 2,
  "name": "Simplify Spotify",
  "description": "Simple Spotify controls + playlist loader right on your extentions bar!",
  "key": "",
  "version": "0.6.4",
  "icons": {
    "16": "icon/icon512.png",
    "48": "icon/icon512.png",
    "128": "icon/icon512.png",
    "512": "icon/icon512.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "js": ["play-playlist.js"],
      "matches": ["https://open.spotify.com/playlist/*"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-inline' 'unsafe-eval' https://sander.vonk.productions/* https://*.spotify.com/* https://*.polyfill.io/* https://spotify-player.herokuapp.com/*; object-src 'self'",
  "permissions": ["identity", "tabs", "storage"]
}
